<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph_home"
    app:startDestination="@id/home_dest">

    <fragment
        android:id="@+id/home_dest"
        android:name="com.med.utilization.bnv.home.HomeFragment"
        android:label="Home"
        tools:layout="@layout/bnv_fragment_home">
        <action
            android:id="@+id/action_home_to_details"
            app:destination="@id/details_dest" />
    </fragment>

    <fragment
        android:id="@+id/details_dest"
        android:name="com.med.utilization.bnv.home.PhotoDetailsFragment"
        android:label="Photo details"
        tools:layout="@layout/bnv_fragment_photo" />

</navigation>